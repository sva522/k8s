[ req ]
default_bits       = 4096
prompt             = no
default_md         = sha512
distinguished_name = dn
req_extensions     = req_ext

[ dn ]
CN = svc.lab.ln

[ req_ext ]
subjectAltName = @alt_names
basicConstraints = CA:FALSE

[ alt_names ]
DNS.1 = svc.lab.ln
DNS.2 = *.svc.lab.ln
DNS.3 = svc
DNS.4 = *.svc
IP.1  = 127.0.0.1
