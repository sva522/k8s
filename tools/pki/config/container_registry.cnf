[ req ]
default_bits       = 4096
prompt             = no
default_md         = sha512
distinguished_name = dn
req_extensions     = req_ext

[ dn ]
CN = container-registry.lab.ln

[ req_ext ]
subjectAltName = @alt_names
basicConstraints = CA:FALSE

[ alt_names ]
DNS.1 = container-registry.lab.ln
DNS.2 = container-registry
IP.1  = 127.0.0.1
